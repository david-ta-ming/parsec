# Your OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI model to use for transformations
# Examples: gpt-4o, gpt-4o-mini, gpt-3.5-turbo
OPENAI_MODEL=gpt-4o-mini

# Optional: Set the log level (error, warn, info, http, debug)
LOG_LEVEL=info

# Optional: Support email address
# If not set, the support link will be hidden in the footer
NEXT_PUBLIC_SUPPORT_EMAIL=name@email.com

# Optional: Google Search Console verification code
NEXT_PUBLIC_GOOGLE_VERIFICATION_CODE=your_verification_code_here

# Optional: Google Analytics ID
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Optional: Set the port for the Next.js development server
# PORT=3000