# Your OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI model to use for transformations
# Examples: gpt-4o, gpt-4o-mini, gpt-3.5-turbo
OPENAI_MODEL=gpt-4o-mini

# Optional: Set the log level (error, warn, info, http, debug)
LOG_LEVEL=info

# Optional: Set the port for the Next.js development server
# PORT=3000